{
  "name": "SpeedLazer",
  "version": "2.1.2-pre",
  "description": "SpeedLazer is a HTML5 Side scrolling shoot-em-up made with Crafty.js",
  "license": "MIT",
  "homepage": "http://speedlazer.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/speedlazer/speedlazer"
  },
  "keywords": [
    "side-scroller",
    "HTML5",
    "Crafty.js",
    "SHMUP",
    "Gamepad",
    "WebGL"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsonschema": "^1.2.6",
    "mocha": "^6.2.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.2.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
    "lint": "eslint src/",
    "test": "mocha --recursive --require @babel/register",
    "prettier": "prettier '{,!(node_modules|dist|.tmp)/**/}*.{js,css,scss,json}' --write"
  },
  "babel": {
    "presets": ["@babel/env"],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "@babel/proposal-class-properties"
    ]
  },
  "dependencies": {
    "Crafty-Distro": "craftyjs/Crafty-Distro#nightlies",
    "bezier-js": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.15",
    "noisejs": "^2.1.0",
    "preact": "^8.5.2",
    "preact-router": "^2.6.1"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      },
      {
        "files": "*.{s,}css",
        "options": {
          "parser": "css",
          "tabWidth": 4
        }
      }
    ],
    "parser": "babel",
    "tabWidth": 2
  },
  "resolutions": {
    "lodash": "4.17.15",
    "set-value": "3.0.1",
    "mixin-deep": "2.0.1",
    "fstream": "1.0.12",
    "tar": "4.4.10",
    "js-yaml": "3.13.1",
    "sshpk": "1.16.1",
    "clean-css": "4.2.1",
    "terser-webpack-plugin": "2.2.3",
    "acorn": "7.1.1"
  }
}
