{
  "name": "SpeedLazer",
  "version": "2.1.6",
  "description": "SpeedLazer is a HTML5 Side scrolling shoot-em-up made with Crafty.js",
  "license": "MIT",
  "homepage": "http://speedlazer.net",
  "source": [
    "src/index.html",
    "src/editor.html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/speedlazer/speedlazer"
  },
  "keywords": [
    "side-scroller",
    "HTML5",
    "Crafty.js",
    "SHMUP",
    "Gamepad",
    "WebGL"
  ],
  "scripts": {
    "build": "parcel build",
    "start": "parcel",
    "lint": "eslint src/",
    "test": "mocha --recursive --require @babel/register"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.10.3",
    "@parcel/transformer-glsl": "2.10.3",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.14.3",
    "jsonschema": "^1.2.6",
    "mocha": "^6.2.0",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "bezier-js": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.21",
    "noisejs": "^2.1.0",
    "preact": "^8.5.2",
    "preact-router": "^2.6.1"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      },
      {
        "files": "*.{s,}css",
        "options": {
          "parser": "css",
          "tabWidth": 4
        }
      }
    ],
    "tabWidth": 2
  },
  "packageManager": "yarn@4.0.2"
}
